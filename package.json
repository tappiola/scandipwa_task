{
    "name": "scandipwa_task",
    "description": "Blank app source-code used by CSA.",
    "version": "0.0.0",
    "scripts": {
        "start": "scandipwa-scripts start",
        "build": "scandipwa-scripts build",
        "postinstall": "scandipwa-scripts link",
        "postupdate": "scandipwa-scripts link"
    },
    "dependencies": {
        "@scandipwa/framework": "0.0.3",
        "@scandipwa/router": "0.0.3",
        "@scandipwa/scandipwa-scripts": "2.2.1",
        "antd": "^4.10.2",
        "styleguide-extension": "file:packages/styleguide-extension"
    },
    "scandipwa": {
        "themeAlias": "Blank",
        "type": "theme",
        "extensions": {
            "@scandipwa/framework": true,
            "@scandipwa/router": true,
            "styleguide-extension": true
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": "@scandipwa"
    },
    "license": "OSL-3.0",
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "less": "^4.1.0",
        "less-loader": "^7.2.1"
    }
}
